{
  "compilerOptions": {
    "sourceMap": true,
    "skipLibCheck": true,
    "strictNullChecks": true,
    "esModuleInterop": true,
    "moduleResolution": "Node",
    "target": "es2022",
    "module": "ESNext",
    "composite": true,
    "declaration": true,
    "isolatedModules": true,
    "allowImportingTsExtensions": true,
    "noEmit": true
  }
}
